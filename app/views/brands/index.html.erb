<main class="container">
  <div class="header">
    <h1>Brands</h1>
    <%= link_to "New Brand", new_brand_path, data: { turbo_frame: dom_id(Brand.new) }, class: "btn btn-primary" %>
  </div>
  <%= turbo_frame_tag Brand.new %>

  <%= turbo_frame_tag "brands" do %>
    <%= render @brands %>
  <% end %>
</main>